    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "if-env": "^1.0.0",
    "preact-cli": "^1.1.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "history": "^4.6.1",
    "moment": "^2.18.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
